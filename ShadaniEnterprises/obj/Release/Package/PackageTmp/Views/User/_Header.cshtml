                                        <p class="m-b-0">Allied Bank</p>
                                        <p class="m-b-0">A/c # 0010026880790016</p>
                                        <p class="m-b-0">Branch Gulistan-e-Johar Shop 7 & 8 Block-19, Karachi</p>
                                    </li>
                              
                                 
                                </ul>
                            </div>
                        </li>
                        <!-- End Messages -->
                    </ul>
                    <!-- User profile and search -->
                    <ul class="navbar-nav my-lg-0">

                        <!-- Search -->
                        <li class="nav-item hidden-sm-down search-box">
                            <a class="nav-link hidden-sm-down text-muted  " href="javascript:void(0)"><i class="ti-search"></i></a>
                            <form class="app-search">
                                <input type="text" class="form-control" placeholder="Search here"> <a class="srh-btn"><i class="ti-close"></i></a>
                            </form>
                        </li>

                        <!-- Profile -->
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-muted  " href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                @if (Session["UserID"] != null)
                                {
                                    <text>
                                        @Session["UserID"].ToString()
                                    </text>
                                }
                            </a>
                            <div class="dropdown-menu dropdown-menu-right animated zoomIn">
                                <ul class="dropdown-user">
                                    <li><a href="/User/UserProfile"><i class="ti-user"></i> Profile</a></li>
                                    <li><a href="/User/InboxMessages"><i class="ti-email"></i> Inbox</a></li>
                                    <li><a href="/User/ChangePassword"><i class="ti-key"></i> Change Password</a></li>
                                    <li><a href="/User/Login"><i class="fa fa-power-off"></i> Logout</a></li>
                                </ul>
                            </div>
                        </li>
                    </ul>
